{
  "name": "immutable-tree",
  "version": "0.0.0",
  "description": "A typescript library to use simple tree structures with immutability as core concept",
  "author": "luisgm",
  "license": "MIT",
  "homepage": "https://github.com/luisgm9095",
  "repository": {
    "type": "git",
    "url": "https://github.com/luisgm9095/immutable-tree"
  },
  "bugs": {
    "url": "https://github.com/luisgm9095/immutable-tree/issues"
  },
  "keywords": ["tree", "immutable", "typescript"],
  "type": "module",
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ignore-path .eslintignore --ext .js,.ts",
    "preview": "vite preview",
    "test": "vitest"
  },
  "devDependencies": {
    "@types/node": "^20.8.10",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "eslint": "^8.53.0",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-dts": "^3.6.3",
    "vitest": "^0.34.6"
  }
}
